# ===========================================
# BankWise Frontend - Environment Configuration
# ===========================================
# Copy this file to .env and update values for your environment
# Note: Vite requires all env variables to be prefixed with VITE_

# ---- API Configuration ----
# Backend API base URL (no trailing slash)
VITE_API_BASE_URL=http://localhost:8091

# ---- WebSocket Configuration ----
# WebSocket URL for real-time notifications (auto-derived from API URL if not set)
VITE_WS_URL=ws://localhost:8091/ws

# ---- App Metadata ----
VITE_APP_NAME=BankWise
VITE_APP_VERSION=1.0.0
